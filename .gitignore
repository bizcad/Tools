**/packages/*
!**/packages/build/
!**/packages/repositories.config

# Logs
*.log

# Test results
TestResults/
*.TestResults/

# Others
*.vspscc
*.vssscc
*.dbmdl
*.bak
*.backup
*.swp
*.tmp
*.orig
*.rej
*.cache
*.pdb
*.mdb
*.opendb
*.VC.db
*.VC.VC.opendb

# Local configuration
local.settings.json

# Python
__pycache__/
*.py[cod]

# Node
node_modules/

# Coverage
coverage/
*.coverage

# JetBrains Rider
.idea/
*.sln.iml

# End of .gitignore
